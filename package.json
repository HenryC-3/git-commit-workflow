	"bugs": {
		"url": "https://github.com/HenryC-3/git-commit-workflow/issues"
	},
	"homepage": "https://github.com/HenryC-3/git-commit-workflow#readme",
	"devDependencies": {
		"@commitlint/cli": "^11.0.0",
		"@commitlint/config-conventional": "^11.0.0",
		"commitizen": "^4.2.3",
		"cz-conventional-changelog": "^3.3.0",
		"husky": "^5.0.6"
	}
